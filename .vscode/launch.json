{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "cds watch --profile hybrid",
      "name": "local",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "SSH",
      "request": "attach",
      "type": "node",
      "port": 9229,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/home/vcap/app"
    }
  ]
}